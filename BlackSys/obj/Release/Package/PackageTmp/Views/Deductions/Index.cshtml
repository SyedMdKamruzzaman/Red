@model IEnumerable<BlackSys.Models.Deduction>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DeductionName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DeductionCategoryId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DeductionType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsManageBySystem)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsPartOfGrossSalary)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        
        <td>
            @Html.DisplayFor(modelItem => item.DeductionName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DeductionCategory)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DeductionType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ManagebySystem)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PartofGrossSalary)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
